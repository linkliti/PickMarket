{
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "hidefiles.configurationType": "local",
  "debug.javascript.terminalOptions": {
    "skipFiles": [
      "<node_internals>/**"
    ]
  },
  "hidefiles.globalConfig": {
    "profiles": [
      {
        "name": "Hide Files",
        "detail": "",
        "description": "Hides configuration/cache files",
        "hidden": [
          // frontend
          "frontend/node_modules",
          "frontend/.eslintrc.cjs",
          "frontend/package*",
          "frontend/*config.*",
          "*/vite.config.ts",
          // backend
          "go.work",
          "**/go.mod",
          // parser
          "parser/venv",
          "**/__pycache__",
          "parser/requirements.txt",
          // global
          ".gitignore",
          ".vscode/",
          ".trunk"
        ]
      }
    ]
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true
  },
  "hidefiles.selectedProfile": "Show All Files",
  "python.testing.pytestArgs": [
    "pmparser",
    "-s"
  ],
  "python.testing.unittestEnabled": false,
  "python.testing.pytestEnabled": true,
  "python.analysis.diagnosticSeverityOverrides": {
    "reportShadowedImports": "none"
  }
}