{
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "hidefiles.configurationType": "local",
  "debug.javascript.terminalOptions": {
    "skipFiles": [
      "<node_internals>/**"
    ]
  },
  "hidefiles.globalConfig": {
    "profiles": [
      {
        "name": "Hide Files",
        "detail": "",
        "description": "Hides configuration/cache files",
        "hidden": [
          // frontend
          "frontend/node_modules",
          "frontend/.eslintrc.cjs",
          "frontend/package*",
          "frontend/*config.*",
          "*/vite.config.ts",
          // backend
          "go.work",
          "**/go.mod",
          // parser
          "pmparser/venv",
          "**/__pycache__",
          "parser/requirements.txt",
          ".style.yapf",
          ".pylintrc",
          // global
          ".gitignore",
          ".vscode/",
          ".pytest_cache",
          "**/*.egg-info"
        ]
      }
    ]
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "frontend/node_modules": true,
    "frontend/.eslintrc.cjs": true,
    "frontend/package*": true,
    "frontend/*config.*": true,
    "*/vite.config.ts": true,
    "go.work": true,
    "**/go.mod": true,
    "pmparser/venv": true,
    "**/__pycache__": true,
    "parser/requirements.txt": true,
    ".style.yapf": true,
    ".pylintrc": true,
    ".gitignore": true,
    ".vscode/": true,
    ".pytest_cache": true,
    "**/*.egg-info": true
  },
  "hidefiles.selectedProfile": "Hide Files",
  "python.testing.unittestEnabled": false,
  "python.testing.pytestEnabled": true,
  "python.analysis.diagnosticSeverityOverrides": {
    "reportShadowedImports": "none"
  },
  "python.analysis.typeCheckingMode": "standard",
  "protoc": {
    "options": [
      "--proto_path=${workspaceRoot}/proto"
    ],
  },
  "pylint.ignorePatterns": [
    "**/site-packages/**/*.py",
    ".vscode/*.py",
    "**/proto/**"
  ],
  "python.analysis.extraPaths": [
    "./pmparser/pmparser"
  ],
  "python.testing.pytestArgs": [
    "pmparser/tests",
    "-s"
  ]
}