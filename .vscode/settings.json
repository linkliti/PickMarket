{
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.quickSuggestions": {
    "strings": "on"
  },
  "hidefiles.configurationType": "local",
  "debug.javascript.terminalOptions": {
    "skipFiles": [
      "<node_internals>/**"
    ]
  },
  "hidefiles.globalConfig": {
    "profiles": [
      {
        "name": "Hide Files",
        "detail": "",
        "description": "Hides configuration/cache files",
        "hidden": [
          "requests_backup",
          "pmparser/venv",
          "parser/requirements.txt",
          "go.work.sum",
          "frontend/tsconfig*",
          "frontend/postcss.config.js",
          "frontend/package*",
          "frontend/node_modules",
          "frontend/components.json",
          "frontend/.eslintrc.cjs",
          "**/vite-env.d.ts",
          "**/go.sum",
          "**/go.mod",
          "**/downloaded_files",
          "**/*.egg-info",
          "**/.pytest_cache",
          "**/__pycache__",
          "**/__debug*",
          ".style.yapf",
          ".pylintrc",
          ".prettierrc",
        ]
      }
    ]
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true
  },
  "hidefiles.selectedProfile": "Show All Files",
  "python.testing.unittestEnabled": false,
  "python.testing.pytestEnabled": true,
  "python.analysis.diagnosticSeverityOverrides": {
    "reportShadowedImports": "none"
  },
  "python.analysis.typeCheckingMode": "standard",
  "protoc": {
    "options": [
      "--proto_path=${workspaceRoot}/proto"
    ],
  },
  "pylint.ignorePatterns": [
    "**/site-packages/**/*.py",
    ".vscode/*.py",
    "**/protos/**"
  ],
  "python.analysis.extraPaths": [
    "./pmparser/app"
  ],
  "python.testing.pytestArgs": [
    "pmparser/tests",
    "-s",
  ],
  "python.analysis.ignore": [
    "pmparser/app/protos"
  ],
  "go.testEnvFile": "${workspaceFolder}/backend/.env",
  "python.envFile": "${workspaceFolder}/pmparser/.env",
  "typescript.preferences.autoImportFileExcludePatterns": [
    "cmdk",
    "@radix-ui"
  ]
}